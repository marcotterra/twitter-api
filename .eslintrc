{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["plugin:prettier/recommended", "eslint:recommended"],
  "rules": {
    "no-console": "warn",
    "no-underscore-dangle": [1, { "allow": ["_id"] }],
    "prettier/prettier": "error"
  }
}
